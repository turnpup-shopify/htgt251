  {% unless product.metafields.pdp.details == blank %}
      <div class="turnpup_specification">
        <div class="wrapper three-column">
          <div class="turnpup_left">
            <a class="pdp_bottom_header" js-accordion-header> 
              <h2>{{ section.settings.bottom_left_section_title }}</h2>
            </a>
            <div class="pdp_lower_cms_section invisible" aria-hidden="true" js-accordion-content>{{ product.metafields.pdp.details }}</div>       
          </div>
          <div class="turnpup_middle">
            <a class="pdp_bottom_header" js-accordion-header>
              <h2>{{ section.settings.bottom_middle_section_title }}</h2>
            </a>
            <div class="pdp_lower_cms_section invisible" aria-hidden="true" js-accordion-content>
              {{ product.metafields.pdp.dimensions }}
              <span id="dimension_read_more_reveal" class="display_none" aria-hidden="true">{{ product.metafields.pdp.dimensions-invisible }}</span>
            </div>
            {%- if product.metafields.pdp.dimensions-invisible -%}
              <a id="dimension_read_more" style="display:inline-block;margin-top: 10px;color: var(--hapny-blue);text-decoration: underline;"> Expand All </a>
            {%- endif -%}
          </div>
          <div class="turnpup_right"> 
            <a class="pdp_bottom_header" js-accordion-header>
              <h2>{{ section.settings.bottom_right_section_title }}</h2>
            </a>
            <div class="pdp_lower_cms_section invisible" aria-hidden="true" js-accordion-content>{{ product.metafields.pdp.downloads }}</div>
          </div>
        </div>
      </div>
      {% endunless %}

